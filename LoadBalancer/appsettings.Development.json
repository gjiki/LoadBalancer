{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "LoadBalancer": "Server=(localdb)\\MSSQLLocalDB;Integrated Security=true;"
  },
  "BalancerSettings": {
    "Hosts": [
      {
        "Server": "https://localhost:6001/",
        "Load": 60
      },
      {
        "Server": "https://localhost:6002/",
        "Load": 20
      },
      {
        "Server": "https://localhost:6003/",
        "Load": 20
      }
    ]
  },
  "BackgroundJobsSettings": {
    "JobSettings": [
      {
        "Name": "PingServers",
        "Delay": 5
      }
    ]
  },
  "AllowedHosts": "*"
}
